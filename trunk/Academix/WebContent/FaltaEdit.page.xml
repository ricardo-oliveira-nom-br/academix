<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd"
	no-conversation-view-id="/FaltaList.xhtml" login-required="true">

	<begin-conversation join="true" />

	<action execute="#{faltaHome.wire}" />

	<param name="faltaFrom" />
	<param name="faltaIdtFalta" value="#{faltaHome.faltaIdtFalta}" />
	<param name="disciplinaFrom" />
	<param name="disciplinaIdtDisciplina"
		value="#{disciplinaHome.disciplinaIdtDisciplina}" />

	<param name="turmaFrom" />
	<param name="turmaIdtTurma" value="#{turmaHome.turmaIdtTurma}" />
	<param name="gradeCurricularFrom" />
	<param name="gradeCurricularIdtGradeCurricular"
		value="#{gradeCurricularHome.gradeCurricularIdtGradeCurricular}" />
	<param name="cursoFrom" />
	<param name="cursoIdtCurso" value="#{cursoHome.cursoIdtCurso}" />
	<param name="campusFrom" />
	<param name="campusIdtCampus" value="#{campusHome.campusIdtCampus}" />
	<param name="enderecoFrom" />
	<param name="enderecoIdtCep" value="#{enderecoHome.enderecoIdtCep}" />





	<param name="alunoFrom" />
	<param name="alunoIdtAluno" value="#{alunoHome.alunoIdtAluno}" />



	<navigation from-action="#{faltaHome.preparePersist}">
		<end-conversation />
		<redirect view-id="/Falta.xhtml" />
	</navigation>

	<navigation from-action="#{faltaHome.prepareUpdate}">
		<end-conversation />
		<redirect view-id="/Falta.xhtml" />
	</navigation>

	<navigation from-action="#{faltaHome.remove}">
		<end-conversation />
		<redirect view-id="/FaltaList.xhtml" />
	</navigation>

	<restrict>
		#{identity.loggedIn and (s:hasRole("professor") or
		s:hasRole("admin"))}
	</restrict>

</page>
