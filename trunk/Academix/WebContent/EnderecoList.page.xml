<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<param name="firstResult" value="#{enderecoList.firstResult}" />
	<param name="order" value="#{enderecoList.order}" />
	<param name="from" />
	<param name="idtCep" value="#{enderecoList.endereco.idtCep}" />
	<param name="desLogradouro"
		value="#{enderecoList.endereco.desLogradouro}" />
	<param name="desBairro" value="#{enderecoList.endereco.desBairro}" />
	<param name="desCidade" value="#{enderecoList.endereco.desCidade}" />
	<param name="desUf" value="#{enderecoList.endereco.desUf}" />

	<restrict>
		#{identity.loggedIn and (s:hasRole("admin") or
		s:hasRole("secretaria"))}
	</restrict>

</page>
