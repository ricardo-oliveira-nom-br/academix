<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<param name="gradeHorariaFrom" />
	<param name="gradeHorariaIdtGradeHoraria"
		value="#{gradeHorariaHome.gradeHorariaIdtGradeHoraria}" />
	<param name="localizacaoFrom" />
	<param name="localizacaoIdtLocalizacao"
		value="#{localizacaoHome.localizacaoIdtLocalizacao}" />

	<param name="disciplinaFrom" />
	<param name="disciplinaIdtDisciplina"
		value="#{disciplinaHome.disciplinaIdtDisciplina}" />

	<param name="turmaFrom" />
	<param name="turmaIdtTurma" value="#{turmaHome.turmaIdtTurma}" />
	<param name="gradeCurricularFrom" />
	<param name="gradeCurricularIdtGradeCurricular"
		value="#{gradeCurricularHome.gradeCurricularIdtGradeCurricular}" />
	<param name="cursoFrom" />
	<param name="cursoIdtCurso" value="#{cursoHome.cursoIdtCurso}" />
	<param name="campusFrom" />
	<param name="campusIdtCampus" value="#{campusHome.campusIdtCampus}" />
	<param name="enderecoFrom" />
	<param name="enderecoIdtCep" value="#{enderecoHome.enderecoIdtCep}" />

	<param name="professorFrom" />
	<param name="professorIdtProfessor"
		value="#{professorHome.professorIdtProfessor}" />

	<action execute="#{gradeHorariaHome.prepareView}"
		if="#{not empty gradeHorariaHome.gradeHorariaIdtGradeHoraria}" />

	<restrict>
		#{identity.loggedIn and (s:hasRole("admin") or
		s:hasRole("secretaria"))}
	</restrict>

