<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd"
	no-conversation-view-id="/HistoricoList.xhtml" login-required="true">

	<begin-conversation join="true" />

	<action execute="#{historicoHome.wire}" />

	<param name="historicoFrom" />
	<param name="historicoIdtHistorico"
		value="#{historicoHome.historicoIdtHistorico}" />
	<param name="cursoFrom" />
	<param name="cursoIdtCurso" value="#{cursoHome.cursoIdtCurso}" />
	<param name="campusFrom" />
	<param name="campusIdtCampus" value="#{campusHome.campusIdtCampus}" />
	<param name="enderecoFrom" />
	<param name="enderecoIdtCep" value="#{enderecoHome.enderecoIdtCep}" />



	<param name="alunoFrom" />
	<param name="alunoIdtAluno" value="#{alunoHome.alunoIdtAluno}" />



	<navigation from-action="#{historicoHome.persist}">
		<end-conversation />
		<redirect view-id="/Historico.xhtml" />
	</navigation>

	<navigation from-action="#{historicoHome.update}">
		<end-conversation />
		<redirect view-id="/Historico.xhtml" />
	</navigation>

	<navigation from-action="#{historicoHome.remove}">
		<end-conversation />
		<redirect view-id="/HistoricoList.xhtml" />
	</navigation>

</page>
