<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd"
	no-conversation-view-id="/EnderecoList.xhtml" login-required="true">

	<begin-conversation join="true" />

	<action execute="#{enderecoHome.wire}" />

	<param name="enderecoFrom" />
	<param name="enderecoIdtCep" value="#{enderecoHome.enderecoIdtCep}" />


	<navigation from-action="#{enderecoHome.persist}">
		<end-conversation />
		<redirect view-id="/Endereco.xhtml" />
	</navigation>

	<navigation from-action="#{enderecoHome.update}">
		<end-conversation />
		<redirect view-id="/Endereco.xhtml" />
	</navigation>

	<navigation from-action="#{enderecoHome.remove}">
		<end-conversation />
		<redirect view-id="/EnderecoList.xhtml" />
	</navigation>

	<restrict>
		#{identity.loggedIn and (s:hasRole("admin") or
		s:hasRole("secretaria"))}
	</restrict>

</page>
