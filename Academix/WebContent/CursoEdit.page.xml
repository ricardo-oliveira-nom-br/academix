<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd"
	no-conversation-view-id="/CursoList.xhtml" login-required="true">

	<begin-conversation join="true" />

	<action execute="#{cursoHome.wire}" />

	<param name="cursoFrom" />
	<param name="cursoIdtCurso" value="#{cursoHome.cursoIdtCurso}" />
	<param name="campusFrom" />
	<param name="campusIdtCampus" value="#{campusHome.campusIdtCampus}" />
	<param name="enderecoFrom" />
	<param name="enderecoIdtCep" value="#{enderecoHome.enderecoIdtCep}" />




	<navigation from-action="#{cursoHome.persist}">
		<end-conversation />
		<redirect view-id="/Curso.xhtml" />
	</navigation>

	<navigation from-action="#{cursoHome.update}">
		<end-conversation />
		<redirect view-id="/Curso.xhtml" />
	</navigation>

	<navigation from-action="#{cursoHome.remove}">
		<end-conversation />
		<redirect view-id="/CursoList.xhtml" />
	</navigation>

	<restrict>
		#{identity.loggedIn and (s:hasRole("admin") or
		s:hasRole("secretaria"))}
	</restrict>

</page>
