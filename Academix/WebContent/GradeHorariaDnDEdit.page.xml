<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd"
	no-conversation-view-id="/GradeHorariaList.xhtml"
	login-required="true">

	<param name="gradeHorariaFrom" />
	<param name="gradeHorariaIdtGradeHoraria"
		value="#{gradeHorariaHome.gradeHorariaIdtGradeHoraria}" />

	<begin-conversation join="true" />

	<action execute="#{gradeHorariaItemHome.wire}" />

	<navigation from-action="#{gradeHorariaHome.preparePersist}">
		<end-conversation />
		<redirect view-id="/GradeHoraria.xhtml" />
	</navigation>

	<navigation from-action="#{gradeHorariaHome.update}">
		<end-conversation />
		<redirect view-id="/GradeHoraria.xhtml" />
	</navigation>

	<navigation from-action="#{gradeHorariaHome.remove}">
		<end-conversation />
		<redirect view-id="/GradeHorariaList.xhtml" />
	</navigation>

	<restrict>
		#{identity.loggedIn and (s:hasRole("admin") or
		s:hasRole("secretaria"))}
	</restrict>

</page>