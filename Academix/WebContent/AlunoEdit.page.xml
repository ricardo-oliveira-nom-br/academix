<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd"
	no-conversation-view-id="/AlunoList.xhtml" login-required="true">

	<begin-conversation join="true" />

	<action execute="#{alunoHome.wire}" />

	<param name="alunoFrom" />
	<param name="alunoIdtAluno" value="#{alunoHome.alunoIdtAluno}" />


	<navigation from-action="#{alunoHome.persist}">
		<end-conversation />
		<redirect view-id="/Aluno.xhtml" />
	</navigation>

	<navigation from-action="#{alunoHome.update}">
		<end-conversation />
		<redirect view-id="/Aluno.xhtml" />
	</navigation>

	<navigation from-action="#{alunoHome.remove}">
		<end-conversation />
		<redirect view-id="/AlunoList.xhtml" />
	</navigation>

	<restrict>
		#{identity.loggedIn and (s:hasRole("admin") or
		s:hasRole("secretaria"))}
	</restrict>

</page>
