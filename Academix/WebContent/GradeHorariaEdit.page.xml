<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.2.xsd"
      no-conversation-view-id="/GradeHorariaList.xhtml"
      login-required="true">

   <begin-conversation join="true" flush-mode="MANUAL"/>

   <action execute="#{gradeHorariaHome.wire}"/>

   <param name="gradeHorariaFrom"/>
   <param name="gradeHorariaIdtGradeHoraria" value="#{gradeHorariaHome.gradeHorariaIdtGradeHoraria}"/>
   <param name="turmaFrom"/>
   <param name="turmaIdtTurma" value="#{turmaHome.turmaIdtTurma}"/>
   <param name="gradeCurricularFrom"/>
   <param name="gradeCurricularIdtGradeCurricular" value="#{gradeCurricularHome.gradeCurricularIdtGradeCurricular}"/>
   <param name="cursoFrom"/>
   <param name="cursoIdtCurso" value="#{cursoHome.cursoIdtCurso}"/>
   <param name="campusFrom"/>
   <param name="campusIdtCampus" value="#{campusHome.campusIdtCampus}"/>
   <param name="enderecoFrom"/>
   <param name="enderecoIdtCep" value="#{enderecoHome.enderecoIdtCep}"/>







   <navigation from-action="#{gradeHorariaHome.persist}">
      <rule if-outcome="persisted">
         <end-conversation/>
         <redirect view-id="/GradeHoraria.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{gradeHorariaHome.update}">
      <rule if-outcome="updated">
         <end-conversation/>
         <redirect view-id="/GradeHoraria.xhtml"/>
      </rule>
   </navigation>

   <navigation from-action="#{gradeHorariaHome.remove}">
      <rule if-outcome="removed">
         <end-conversation/>
         <redirect view-id="/GradeHorariaList.xhtml"/>
      </rule>
   </navigation>

</page>
