<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd"
	no-conversation-view-id="/SolicitacaoList.xhtml"
	login-required="true">

	<begin-conversation join="true" />

	<action execute="#{solicitacaoHome.wire}" />

	<param name="solicitacaoFrom" />
	<param name="solicitacaoIdtSolicitacao"
		value="#{solicitacaoHome.solicitacaoIdtSolicitacao}" />
	<param name="alunoFrom" />
	<param name="alunoIdtAluno" value="#{alunoHome.alunoIdtAluno}" />



	<navigation from-action="#{solicitacaoHome.persist}">
		<end-conversation />
		<redirect view-id="/Solicitacao.xhtml" />
	</navigation>

	<navigation from-action="#{solicitacaoHome.teste}">
		<end-conversation />
		<redirect view-id="/Solicitacao.xhtml" />
	</navigation>

	<navigation from-action="#{solicitacaoHome.update}">
		<end-conversation />
		<redirect view-id="/Solicitacao.xhtml" />
	</navigation>

	<navigation from-action="#{solicitacaoHome.remove}">
		<end-conversation />
		<redirect view-id="/SolicitacaoList.xhtml" />
	</navigation>

	<restrict>
		#{identity.loggedIn and (s:hasRole("admin") or
		s:hasRole("secretaria"))}
	</restrict>

</page>
